{
    "name": "dnd-playlist-server",
    "version": "1.0.0",
    "description": "Typescript and Git Boilerplate",
    "scripts": {
        "buildBak": "tsc",
        "typeTest": "tsc --noEmit",
        "bundle:createList": "gulp bundleCreateList",
        "dev:server": "nodemon --watch \"./src/**/*\" --exec \"ts-node src/routes/app.ts\"",
        "dev:watch": "gulp watchBundle",
        "dev": "npm run build && npm run bundle:createList && concurrently \"npm:dev:server\" \"npm:dev:watch\"",
        "build": "bash ./bin/build.sh && npx gulp bundle:createList",
        "start": "node dist/routes/app.js",
        "urlCheck": "node dist/agents/loadUrl",
        "push": "echo 'Cleaning Dist' && rm ./dist/* -r && echo 'Building' && (tsc || echo 'TSC build failed') && echo 'Pushing to GitLab' && git push origin master && echo 'Complete'",
        "lint": "eslint --ext .ts ./src",
        "newBranch": "echo Creating Branch && gulp createBranch",
        "pullRequest": "echo Pull Request && gulp createPullRequest",
        "update": "echo Pulling Master && gulp pullMaster",
        "prune": "echo Pruning && gulp prune",
        "getCounts": "node dist/agents/getListCounts",
        "fetchLists": "node dist/utilities/fetchPlaylists"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/grizzgrizz87/dnd-playlist-server.git"
    },
    "keywords": [
        "CSV",
        "Regex"
    ],
    "author": "Joshua Robinson",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/grizzgrizz87/dnd-playlist-server/issues"
    },
    "homepage": "https://gitlab.com/grizzgrizz87/dnd-playlist-server#README",
    "dependencies": {
        "@codegenie/serverless-express": "^4.14.1",
        "@reduxjs/toolkit": "^2.6.0",
        "async": "^3.2.5",
        "atob": "^2.1.2",
        "aws-lambda": "^1.0.7",
        "axios": "^1.7.2",
        "axios-cookiejar-support": "^5.0.2",
        "btoa": "^1.2.1",
        "cors": "^2.8.5",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "edit-json-file": "^1.8.0",
        "express": "^4.18.2",
        "googleapis": "^117.0.0",
        "jsdom": "^21.1.1",
        "JSONStream": "^1.3.5",
        "opn": "^5.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.2.0",
        "semantic-ui-css": "^2.5.0",
        "semantic-ui-react": "^2.1.5"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/debug": "4.1.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/node": "^20.17.19",
        "@types/react": "^18.0.35",
        "@types/react-dom": "^18.0.11",
        "@types/react-redux": "^7.1.34",
        "@types/youtube": "^0.0.47",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "aws-cdk": "^2.145.0",
        "aws-cdk-lib": "^2.145.0",
        "concurrently": "^8.2.2",
        "esbuild": "^0.27.3",
        "eslint": "^8.38.0",
        "gulp": "^5.0.0",
        "gulp-cli": "^3.0.0",
        "gulp-prompt": "^1.2.0",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.1.4",
        "jest": "^29.5.0",
        "jest-junit": "^15.0.0",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.2",
        "performance": "^1.4.0",
        "performance-now": "^2.1.0",
        "sequelize": "^6.35.2",
        "sqlite3": "^5.1.6",
        "ts-jest": "^29.1.4",
        "ts-node": "^10.9.2",
        "typedoc": "^0.25.13",
        "typescript": "^5.4.5"
    }
}

{
    "name": "ts-git-boilerplate",
    "version": "1.0.0",
    "description": "Typescript and Git Boilerplate",
    "main": "index.js",
    "scripts": {
        "urlCheck": "node dist/agents/loadUrl",
        "push": "echo 'Cleaning Dist' && rimraf ./.dist/* -r && echo 'Building' && (tsc || echo 'TSC build failed' --success = first) && echo 'Pushing to GitLab' && git push origin master && echo 'Complete'",
        "lint": "eslint --ext .ts ./.src",
        "build": "echo 'Cleaning Dist' && rimraf ./.dist/* -r && echo 'Building' && (tsc || echo 'TSC build failed' --success = first) && echo 'Complete'",
        "newBranch": "gulp createBranch",
        "pullRequest": "gulp createPullRequest",
        "update": "gulp pullMaster",
        "prune": "gulp prune",
        "testAuth": "node dist/helpers/oosConnect",
        "testUrlLoader": "node dist/utilities/urlLoader"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/grizzgrizz87/ts-git-boilerplate.git"
    },
    "keywords": [
        "CSV",
        "Regex"
    ],
    "author": "Joshua Robinson",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/grizzgrizz87/ts-git-boilerplate/issues"
    },
    "homepage": "https://gitlab.com/grizzgrizz87/ts-git-boilerplate#README",
    "dependencies": {
        "async": "^2.5.0",
        "atob": "^2.1.2",
        "btoa": "^1.2.1",
        "csv-parse": "^1.2.3",
        "debug": "^3.1.0",
        "excel4node": "^1.7.2",
        "fast-csv": "^2.4.1",
        "googleapis": "^37.2.0",
        "jsdom": "^15.1.1",
        "JSONStream": "^1.3.5",
        "performance": "^1.1.1",
        "performance-now": "^2.1.0",
        "unirest": "^0.6.0"
    },
    "devDependencies": {
        "@types/async": "~2.0.44",
        "@types/chai": "^4.0.4",
        "@types/chai-http": "^3.0.3",
        "@types/csv-parse": "^1.1.12",
        "@types/debug": "0.0.30",
        "@types/mocha": "^2.2.43",
        "@types/request": "^2.47.1",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "gulp": "^4.0.2",
        "gulp-prompt": "^1.2.0",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.1.3",
        "jest": "^22.4.3",
        "mocha": "^3.5.3",
        "opn": "^5.3.0",
        "ts-node": "^3.3.0",
        "tslint": "^5.19.0",
        "typedoc": "^0.15.0",
        "typescript": "^4.7.4"
    }
}

{
    "name": "dnd-playlist-server",
    "version": "1.0.0",
    "description": "Typescript and Git Boilerplate",
    "scripts": {
        "build": "echo 'Cleaning Dist' && rimraf ./dist/* -r && echo 'Building' && (tsc || echo 'TSC build failed' --success=first) && echo 'Complete'",
        "dev": "nodemon --watch \"./src/**/*\" --exec \"ts-node\" src/routes/app.ts",
        "start": "node dist/routes/app.js",
        "urlCheck": "node dist/agents/loadUrl",
        "push": "echo 'Cleaning Dist' && rimraf ./dist/* -r && echo 'Building' && (tsc || echo 'TSC build failed' --success = first) && echo 'Pushing to GitLab' && git push origin master && echo 'Complete'",
        "lint": "eslint --ext .ts ./src",
        "newBranch": "gulp createBranch",
        "pullRequest": "gulp createPullRequest",
        "update": "gulp pullMaster",
        "prune": "gulp prune",
        "getCounts": "node dist/agents/getListCounts",
        "fetchLists": "node dist/utilities/fetchPlaylists"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/grizzgrizz87/dnd-playlist-server.git"
    },
    "keywords": [
        "CSV",
        "Regex"
    ],
    "author": "Joshua Robinson",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/grizzgrizz87/dnd-playlist-server/issues"
    },
    "homepage": "https://gitlab.com/grizzgrizz87/dnd-playlist-server#README",
    "dependencies": {
        "async": "^3.2.4",
        "atob": "^2.1.2",
        "axios": "^1.3.5",
        "axios-cookiejar-support": "^4.0.6",
        "btoa": "^1.2.1",
        "cors": "^2.8.5",
        "csv-parse": "^5.3.6",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "edit-json-file": "^1.7.0",
        "express": "^4.18.2",
        "fast-csv": "^4.3.6",
        "googleapis": "^117.0.0",
        "jest-junit": "^15.0.0",
        "jsdom": "^21.1.1",
        "JSONStream": "^1.3.5",
        "performance": "^1.4.0",
        "performance-now": "^2.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "semantic-ui-css": "^2.5.0",
        "semantic-ui-react": "^2.1.5",
        "ts-jest": "^29.1.0"
    },
    "devDependencies": {
        "@types/async": "^3.2.18",
        "@types/cors": "^2.8.13",
        "@types/csv-parse": "^1.1.12",
        "@types/debug": "4.1.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.35",
        "@types/react-dom": "^18.0.11",
        "@types/request": "^2.48.8",
        "@types/youtube": "^0.0.47",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "concurrently": "^8.0.1",
        "eslint": "^8.38.0",
        "gulp": "^4.0.2",
        "gulp-prompt": "^1.2.0",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.1.4",
        "jest": "^29.5.0",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.2",
        "sequelize": "^6.35.2",
        "sqlite3": "^5.1.6",
        "ts-node": "^10.9.1",
        "typedoc": "^0.24.1"
    }
}
